from Bio.Blast import NCBIWWW
from Bio import SeqIO
record = SeqIO.read(open("NOTCH2.fasta"), format="fasta")
result_handle = NCBIWWW.qblast("blastn", "nt", record.format("fasta"))

save_file = open("my_blast1.xml", "w")
save_file.write(result_handle.read())
save_file.close()
result_handle.close()

from Bio.Blast import NCBIWWW
from Bio import SeqIO
record = SeqIO.read(open("PTGFRN.fasta"), format="fasta")
result_handle = NCBIWWW.qblast("blastn", "nt", record.format("fasta"))

save_file = open("my_blast2.xml", "w")
save_file.write(result_handle.read())
save_file.close()
result_handle.close()

